{
  "name": "Course",
  "plural": "courses",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "instructor": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "students": {
      "type": "hasMany",
      "model": "Student",
      "foreignKey": "course",
      "through": "Registration",
      "keyThrough": "student"
    },
    "courseRatings": {
      "type": "hasMany",
      "model": "CourseRating",
      "foreignKey": "course"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
